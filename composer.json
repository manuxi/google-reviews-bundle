{
    "name": "manuxi/google-reviews-bundle",
    "description": "Bundle to get Google reviews easily",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "manuxi",
            "email": "manu@bytefresser.de"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "symfony/config": "^4.0 || ^5.0 || ^6.0",
        "symfony/dependency-injection": "^4.0 || ^5.0 || ^6.0",
        "symfony/http-kernel": "^4.0 || ^5.0 || ^6.0",
        "symfony/cache": "^4.0 || ^5.0 || ^6.0",
        "symfony/serializer": "^4.0 || ^5.0 || ^6.0",
        "symfony/yaml": "^4.0 || ^5.0 || ^6.0",
        "symfony/framework-bundle": "^4.0 || ^5.0 || ^6.0",
        "symfony/twig-bundle": "^4.0 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.6",
        "symfony/property-access": "^4.0 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8",
        "symfony/debug-bundle": "^4.0 || ^5.0 || ^6.0",
        "symfony/phpunit-bridge": "^4.0 || ^5.0 || ^6.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\GoogleReviewsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\GoogleReviewsBundle\\Tests\\": "tests/"
        }
    }
}
